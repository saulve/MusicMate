{
  "name": "musicMate",
  "short_name": "musicMate",
  "description":
    "Effortless discovery of performances from your favourite artists",
  "version": "",
  "manifest_version": 2,
  "key":
    "",
  "icons": {
    "48": "icons/music-mate-48.png",
    "128": "icons/music-mate-128.png"
  },
  "browser_action": {
    "default_title": "musicMate",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "https://accounts.spotify.com/authorize",
    "https://*.spotify.com/*",
    "https://i.scdn.co/*",
    "notifications",
    "background",
    "storage",
    "management",
    "webRequest",
    "webRequestBlocking",
    "identity"
  ]
}
